{
  "name": "tetris",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "deploy": "npm run build:style && npm run build:webpack",
    "play:console": "tsc --module commonjs && node dist/console.js",
    "play:web": "npm run deploy && cd ./dist/ && http-server -o ./",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "build:webpack": "webpack",
    "watch:webpack": "webpack --watch",
    "build:style": "node-sass src/style.scss dist/style.css",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/app.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.1.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/vue": "^2.0.0",
    "@webpack-cli/init": "^1.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.0",
    "node-sass": "^9.0.0",
    "nodemon": "^2.0.2",
    "terser-webpack-plugin": "^2.3.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^5.2.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-loader": "^6.2.1",
    "vue": "^2.6.11"
  }
